@page
@model FandomWiki.Pages.CommunitySearchResultModel
@{
    Layout = "_IndexLayout";
}

<link rel="stylesheet" href="~/css/communitySearchResult.css" asp-append-version="true" />
<searchResultTable>
    @foreach(var community in Model.Communities) {
        <searchResultBlock>
            <communityName>@community.Name</communityName>
            <br />
            <communityDescription>@community.Description</communityDescription>
        </searchResultBlock>
    }
</searchResultTable>
